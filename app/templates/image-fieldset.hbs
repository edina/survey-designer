<fieldset class="fieldcontain fieldcontain-image" id="{{fieldId}}">
  <legend>{{t "image.field-title"}}</legend>
  <label for="label">{{t "image.label-title"}}</label>
  <input type="text" name="label" value="{{label}}">
  <input type="checkbox" name="required" {{check required "checked"}}>{{t "image.required"}}
  <div class="form-inline">
    <input type="checkbox" name="multi-image" {{check multi-image "checked"}}>{{t "image.multiple-images"}}
  </div>
  <div class="form-inline">
    <input type="checkbox" name="los" {{check los "checked"}}>{{t "image.los"}}
  </div>
  <div class="form-inline">
</fieldset>