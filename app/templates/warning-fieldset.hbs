<fieldset class="fieldcontain fieldcontain-warning" id="{{id}}" data-type="{{type}}">
  <legend>{{t "warning.field-title"}}</legend>
  <label for="label">{{t "warning.label-title"}}</label>
  <input type="text" name="label" value="{{label}}">
  <label for="message">{{t "warning.label"}}</label>
  <textarea placeholder="{{t "warning.message"}}">{{properties.placeholder}}</textarea>
</fieldset>
