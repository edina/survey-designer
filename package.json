{
  "name": "survey-designer",
  "version": "0.0.0",
  "description": "Create custom forms",
  "main": "index.js",
  "scripts": {
    "lint": "node_modules/jshint/bin/jshint src/www/js/**.js",
    "release": "npm run lint && node_modules/release-it/bin/release.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node_modules/beefy/bin/beefy app/app.js:app/bundle.js --live --cwd app -- -t [node-underscorify --extensions html]",
    "bundle": "browserify -t [node-underscorify --extensions html] app/app.js -o app/bundle.js",
    "watch": "watchify -t [node-underscorify --extensions html] app/app.js -o app/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edina/survey-designer.git"
  },
  "keywords": [
    "fieldtrip",
    "survey",
    "designer"
  ],
  "author": "Panos Terzis",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/edina/survey-designer/issues"
  },
  "homepage": "https://github.com/edina/survey-designer",
  "dependencies": {},
  "devDependencies": {
    "beefy": "^2.1.5",
    "jshint": "^2.7.0",
    "release-it": "0.0.15",
    "uglify": "^0.1.3"
  },
  "jspm": {
    "directories": {
      "baseURL": "app"
    },
    "dependencies": {
      "backbone": "npm:backbone@^1.2.0",
      "bootstrap": "github:twbs/bootstrap@^3.3.4",
      "css": "github:systemjs/plugin-css@^0.1.12",
      "handlebars": "github:components/handlebars.js@^3.0.3",
      "hbs": "github:davis/plugin-hbs@master",
      "i18next": "npm:i18next@^1.9.0",
      "jquery": "github:components/jquery@^2.1.4",
      "jquery-ui": "github:components/jqueryui@^1.11.4",
      "pcapi": "npm:pcapi@^0.0.7",
      "underscore": "npm:underscore@^1.8.3",
      "x-editable": "npm:x-editable@^1.5.1"
    },
    "devDependencies": {
      "traceur": "github:jmcriffey/bower-traceur@0.0.88",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.88"
    }
  }
}
